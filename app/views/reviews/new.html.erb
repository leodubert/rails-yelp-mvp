
<div>
<h1>Add a review !</h1>
<%= simple_form_for([@restaurant, @review]) do |f| %>
 <%= f.error_notification %>
  <%= f.input :content %>
  <%= f.input :rating, collection: Review::RATINGS %>
</div>

<div class="form-actions", class= "btn btn-primary" >
<%= f.button :submit %>
</div>



<% end %>
